   "source": [
    "#Inheritence\n",
    "\n",
    "class Animal:\n",
    "    def leg(self):\n",
    "        print(\"Animal has 4 legs\")\n",
    "\n",
    "class Cow(Animal):\n",
    "    def color(self):\n",
    "        print(\"Cow is white in color\")\n",
    "\n",
    "\n",
    "bessie = Cow()\n",
    "# Calling the inherited method from Animal class\n",
    "bessie.leg()\n",
    "bessie.color()\n",
    "\n",
    "\n"
   ]
  }
 ],
